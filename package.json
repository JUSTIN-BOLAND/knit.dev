{
  "name": "knit.dev",
  "author": "tatommyttmy",
  "bugs": "https://github.com/knit-dev/knit.dev/issues",
  "homepage": "https://knit.dev",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Bespoke web and mobile app development for your startup. Design, build, deploy, and scale. Knit, code as craft.",
  "repository": {
    "type": "git",
    "url": "https://github.com/knit-dev/knit.dev"
  },
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "devtools": "./node_modules/.bin/vue-devtools",
    "dev-devtools": "npm run devtools & env VUE_DEVTOOLS_LOCAL=true npm run dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@aceforth/nuxt-optimized-images": "^1.0.1",
    "@mdi/js": "^5.3.45",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/sentry": "^4.1.2",
    "@types/swiper": "^5.3.2",
    "@vue/composition-api": "^0.6.3",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-svgo": "^8.0.0",
    "nuxt": "^2.0.0",
    "swiper": "^5.4.2",
    "typeface-poppins": "0.0.72",
    "typeface-space-mono": "0.0.71",
    "vue-awesome-swiper": "^4.1.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/devtools": "^5.3.3",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.0.1",
    "chalk": "^4.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.10",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.0",
    "ts-jest": "^26.1.0",
    "vue-jest": "^4.0.0-0"
  }
}
